<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:orientation="vertical"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:background="@drawable/img_bg01"
              android:gravity="center_horizontal"
    >
    <TextView
        android:id="@+id/total_overtime_approve"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="全部加班审批"
        android:layout_marginTop="20dp"
        />
    <TextView
        android:id="@+id/total_leave_approve"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="全部请假审批"
        android:layout_marginTop="25dp"
        />
    <TextView
        android:id="@+id/total_outside_approve"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="全部外勤/出差审批"
        android:layout_marginTop="25dp"
        />
    <TextView
        android:id="@+id/total_reimburse_approve"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="全部报销审批"
        android:layout_marginTop="25dp"
        />
    <TextView
        android:id="@+id/total_common_approve"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="全部通用审批"
        android:layout_marginTop="25dp"
        />
</LinearLayout>